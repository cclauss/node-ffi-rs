{
  "name": "ffi-rs",
  "version": "1.0.3",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "A module written in Rust and N-API provides interface (FFI) features for Node.js",
  "napi": {
    "name": "ffi-rs",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "author": "zhangyuang",
  "homepage": "https://github.com/zhangyuang/node-ffi-rs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangyuang/node-ffi-rs.git"
  },
  "keywords": [
    "ffi",
    "rust",
    "node.js",
    "napi"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "README.md"
  ],
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.15.2",
    "koa": "^2.14.2",
    "shelljs": "^0.8.5"
  },
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build:c": "node scripts/cpp.js",
    "build": "yarn build:c && napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node ./test.js",
    "universal": "napi universal",
    "version": "napi version"
  },
  "dependencies": {}
}
